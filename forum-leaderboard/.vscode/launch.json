  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/leaderboard.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug leaderboard.js",
      "program": "${workspaceFolder}/leaderboard.js",
      "skipFiles": [
        "<node_internals>/**", // Skip Node.js internals
        "node_modules/**" // Skip all external library code
      ]
    }
  ]
}